# Supabase Configuration
# Lovable Supabase (Default - Production)
VITE_SUPABASE_URL=your_lovable_supabase_url
VITE_SUPABASE_PUBLISHABLE_KEY=your_lovable_supabase_anon_key

# Local Development Supabase (Optional - Override)
VITE_LOCAL_SUPABASE_URL=your_local_supabase_url
VITE_LOCAL_SUPABASE_PUBLISHABLE_KEY=your_local_supabase_anon_key

# Environment Configuration
VITE_USE_LOCAL_SUPABASE=false

# Admin (optional; required for admin panel access)
VITE_ADMIN_EMAIL=admin@example.com
# Dev-only quick sign-in (only set in local .env, never commit)
# VITE_ADMIN_DEV_EMAIL=
# VITE_ADMIN_DEV_PASSWORD=

# Python Scripts Configuration (for scraper)
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_KEY=your_service_role_key
SCRAPERAPI_KEY=your_scraperapi_key
